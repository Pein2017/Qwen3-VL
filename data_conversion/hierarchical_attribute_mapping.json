{
  "metadata": {
    "version": "5.0",
    "description": "Precise hierarchical attribute mapping with exact keys from user specification",
    "language": "chinese",
    "separator_rules": {
      "same_level": "Use ',' to separate values at the same hierarchical level",
      "different_levels": "Use '/' to separate different attribute levels",
      "conditional_attributes": "Only include when parent condition is met",
      "free_text": "Insert directly without modification"
    },
    "geometry_constraints": {
      "line_objects": [
        "fiber",
        "wire"
      ],
      "quad_bbox_objects": [
        "bbu",
        "bbu_shield",
        "connect_point",
        "label"
      ]
    }
  },
